{
  "name": "whatfees",
  "version": "1.0.11",
  "description": "whatfees PWA",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build && node scripts/obfuscate.mjs",
    "preview": "vite preview",
    "test": "node tests/calculations.test.ts && node tests/google-auto-login.test.ts",
    "security:scan": "node scripts/security-scan.mjs",
    "typecheck": "tsc --noEmit",
    "verify": "npm run security:scan && npm run test && npm run typecheck && npm run build",
    "assetlinks": "node scripts/generate-assetlinks.mjs",
    "release:play": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/release-google-play.ps1",
    "api:install": "npm --prefix apps/api install",
    "api:build": "npm --prefix apps/api run build",
    "api:start": "npm --prefix apps/api run start:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unschoolers/WhatFees.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Unschoolers/WhatFees/issues"
  },
  "homepage": "https://github.com/Unschoolers/WhatFees#readme",
  "dependencies": {
    "chart.js": "^4.5.1",
    "vue": "^3.5.28",
    "vuetify": "^3.11.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.4",
    "javascript-obfuscator": "^5.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-vuetify": "^2.1.3"
  }
}
